{
  "name": "watchbeeforvmix",
  "version": "1.0.1",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "build:win": "npx nexe index.js -t windows-22.21.1 --output=dist/watchbeeforvmixWin -r node_modules/**/* --input=package.json --remote=https://github.com/dr-viant/WatchBeeForVmix/releases/download/windows-1.0/",
    "build:linux": "npx nexe index.js --target linux-x64-22.21.1 --output=dist/watchbeeforvmixLin -r node_modules/**/* --input=package.json --python=/usr/bin/python3 --build",
    "build:mac": "npx nexe index.js -t alpine-22.21.1 --output=dist/watchbeeforvmixMac -r node_modules/**/* --input=package.json --python=/usr/bin/python3 --build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch"
  },
  "nexe": {
    "assets": [
      "node_modules/**/*"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.9",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.7",
    "fs": "^0.0.1-security",
    "nock": "^14.0.10",
    "vitest": "^4.0.16"
  },
  "devDependencies": {
    "nexe": "^5.0.0-beta.4"
  }
}
